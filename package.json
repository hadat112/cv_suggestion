{
  "name": "cv-suggestion",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "[ \"$NODE_ENV\" = production ] && exit 0; husky install",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "predev": "cross-env NODE_ENV=development ts-node --project ./tsconfig.node.json ./scripts/genAntdCss.tsx",
    "build": "NODE_ENV=production next build --no-lint",
    "prebuild": "cross-env NODE_ENV=production ts-node --project ./tsconfig.node.json ./scripts/genAntdCss.tsx",
    "start": "next start",
    "types-check": "tsc --pretty --noEmit",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@svgr/webpack": "^6.5.1",
    "@tanstack/react-query": "^4.24.6",
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "antd": "^5.16.2",
    "axios": "^1.3.3",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.11",
    "lodash": "^4.17.21",
    "next": "^13.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.6",
    "typescript": "^5.1.6",
    "yup": "^1.4.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@ant-design/static-style-extract": "^1.0.2",
    "@biomejs/biome": "1.7.3",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "sass": "^1.58.2",
    "stylelint": "^15.1.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^7.0.1",
    "ts-node": "^10.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss}": [
      "biome check --staged --apply"
    ]
  },
  "engines": {
    "node": ">=18.17.1"
  }
}
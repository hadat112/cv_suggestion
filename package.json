{
  "name": "titan",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "[ \"$NODE_ENV\" = production ] && exit 0; husky install",
    "dev": "cross-env NODE_OPTIONS='--inspect' APP_VERSION=\"$(date +'%Y-%m-%d %H:%M:%S')\" next dev",
    "predev": "cross-env NODE_ENV=development ts-node --project ./tsconfig.node.json ./scripts/genAntdCss.tsx",
    "build": "APP_VERSION=$(date +'%Y-%m-%d %H:%M:%S') NODE_ENV=production next build --no-lint",
    "prebuild": "cross-env NODE_ENV=production ts-node --project ./tsconfig.node.json ./scripts/genAntdCss.tsx",
    "start": "next start",
    "types-check": "tsc --pretty --noEmit",
    "format": "prettier --write --list-different \"src/**/*+(js|jsx|ts|tsx|scss)\"",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@svgr/webpack": "^6.5.1",
    "@tanstack/react-query": "^4.24.6",
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "antd": "^5.6.1",
    "axios": "^1.3.3",
    "chart.js": "^4.4.1",
    "classnames": "^2.3.2",
    "exceljs": "^4.4.0",
    "json-bigint": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "next": "^13.4.19",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.5.3",
    "react-number-format": "^5.3.1",
    "swiper": "^11.1.1",
    "tailwindcss": "^3.2.6",
    "typescript": "^5.1.6",
    "yup": "^1.3.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@ant-design/static-style-extract": "^1.0.2",
    "@types/json-bigint": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "sass": "^1.58.2",
    "stylelint": "^15.1.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^7.0.1",
    "ts-node": "^10.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss}": [
      "npm run format"
    ],
    "*.{css,scss}": [
      "npx stylelint"
    ],
    "*.{js,jsx,ts,tsx}": [
      "npx eslint -c .eslintrc.json --fix"
    ]
  },
  "engines": {
    "node": ">=18.17.1"
  }
}
